<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE jboss PUBLIC
      "-//JBoss//DTD JBOSS 4.2//EN"
      "http://www.jboss.org/j2ee/dtd/jboss_4_2.dtd">
<jboss>
	<enterprise-beans>
		<session>
			<ejb-name>AgencyBean</ejb-name>
			<jndi-name>ejb/Agency</jndi-name>
			<resource-ref>
				<res-ref-name>jdbc/Agency</res-ref-name>
				<jndi-name>java:agencyDS</jndi-name>
			</resource-ref>
		</session>
		<session>
			<ejb-name>AdvertiseJobBean</ejb-name>
			<jndi-name>ejb/AdvertiseJob</jndi-name>
			<resource-ref>
				<res-ref-name>jdbc/Agency</res-ref-name>
				<jndi-name>java:agencyDS</jndi-name>
			</resource-ref>
		</session>
		<session>
			<ejb-name>AdvertiseBean</ejb-name>
			<jndi-name>ejb/Advertise</jndi-name>
			<resource-ref>
				<res-ref-name>jdbc/Agency</res-ref-name>
				<jndi-name>java:agencyDS</jndi-name>
			</resource-ref>
		</session>
	</enterprise-beans>
</jboss>